{
  "manifest_version": 2,

  "name": "Scroll Helper",
  "description": "This extension will help you with navigation on the large articles while you scrolling.",
  "version": "0.2.2.8",

  "icons": {
    "128": "icon-origin.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": [ "scroll-helper.js"]
    }
  ],
  "browser_action": {
    "default_name": "Scroll Helper",
    "default_icon": "icon.png"
  }
}